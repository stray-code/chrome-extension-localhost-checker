{
  "manifest_version": 3,
  "name": "localhostチェッカー",
  "description": "閲覧ページがローカルサーバーかどうかが分かります",
  "version": "1.0.0",
  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["src/content/main.js"],
      "matches": ["http://localhost/*", "https://localhost/*"]
    }
  ]
}
